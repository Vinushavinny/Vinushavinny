name = input("Enter your name:")
print("Hi!", name)
for i in range(10000):
    chat = input("Ask some questions")
    if chat == "What is your name:":
        print("My name is Vinusha.")
    elif chat == "What is your favourite colour:":
        print("My favourite colour is black")
    elif chat == "Who made you":
        print("Alexa made me")
    elif chat == "What is your aim":
        print("Clarifying your doubts")
        
